$TTL 1h
@   IN  SOA ns1.milliways.lan. admin.milliways.lan. (
        2025072901 ; Serial
        1h         ; Refresh
        15m        ; Retry
        1w         ; Expire
        1h )       ; Negative Cache TTL

    IN  NS  ns1.milliways.lan.

{% for host, ip in dns_records.items() %}
{{ host }}   IN  A   {{ ip }}
{% endfor %}
